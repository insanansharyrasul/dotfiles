# Add to ~/.config/wofi/config
[drun]
show-icons=true
icon_size=32
allow_images=true
image_size=32
display_generic=true

key_down=Down
key_up=Up
key_left=Left
key_right=Right
key_forward=Tab
key_backward=ISO_Left_Tab
key_submit=Return,KP_Enter
key_exit=Escape
key_pgup=ctrl+b,Prior
key_pgdn=ctrl+f,Next
key_home=g,Home
key_end=G,End
key_delete_word=ctrl+w
key_delete_line=ctrl+u