{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf115",
            "folder_separator_icon": "/",
            "home_icon": "\ueb06",
            "style": "full"
          },
          "template": " <#10141c>\uf07b \uea9c</> {{ .Path }} ",
          "foreground": "#10141c",
          "powerline_symbol": "\ue0b0",
          "leading_powerline_symbol": "\ue0b6",
          "background": "#e6b450",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": " <#10141c>\ue0a0 </>",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": " âžœ ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "foreground": "#10141c",
          "powerline_symbol": "\ue0b0",
          "background": "#f07178",
          "type": "git",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#10141c",
          "powerline_symbol": "\ue0b0",
          "background": "#aad94c",
          "type": "node",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ue3bf ",
          "foreground": "#f07178",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#e6b450>></>",
          "foreground": "#d2a6ff",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}